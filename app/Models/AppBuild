<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Model;

class AppBuild extends Model
{
    protected $fillable = [
        'version_code',
        'version_name',
        'apk_path',
        'sha256',
        'mandatory',
        'release_notes',
        'is_active',
    ];

    protected $casts = [
        'mandatory' => 'boolean',
        'is_active' => 'boolean',
    ];
}