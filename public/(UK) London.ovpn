client
dev tun
proto udp
remote 83.136.254.231 1194
resolv-retry infinite
nobind
persist-key
persist-tun
explicit-exit-notify
auth-user-pass
remote-cert-tls server
auth SHA256
cipher AES-256-CBC
tls-version-min 1.2
reneg-sec 0
ignore-unknown-option block-outside-dns
setenv opt block-outside-dns
verb 3

<auth-user-pass>
{{USERNAME}}
{{PASSWORD}}
</auth-user-pass>

# CA cert and TLS key blocks are appended dynamically by GenerateOvpnFile job


